<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="org.omaps.view" android:versionCode="1" android:versionName="1.0">
	<uses-sdk android:minSdkVersion="10" />

	<application android:icon="@drawable/icon" android:label="@string/app_name">
		<meta-data android:name="com.google.android.backup.api_key"
			android:value="AEdPqrEAAAAISNzyIGtOTLxFGZLFBOs3bsb-TpTonVsA-u9pqQ" />
		<activity android:name=".Splashscreen" android:label="@string/app_name">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name=".Camspy"></activity>
		<activity android:name=".CamspyMenu"></activity>
